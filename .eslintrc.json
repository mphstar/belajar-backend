{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "linebreak-style":"off",
        "eqeqeq": "off",
        "indent": "off",
        "max-len": "off",
        "no-console": "off",
        "operator-linebreak": "off"
    }
}
